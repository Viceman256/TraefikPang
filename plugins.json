{
  "name": "rewriteheader",
  "displayName": "Rewrite Header",
  "type": "HTTP",
  "import": "github.com/mrdrelar/traefik-plugin-rewriteheader",
  "moduleName": "traefik-plugin-rewriteheader",
  "summary": "A Traefik plugin to rewrite request headers.",
  "configuration": {
    "rewrites": [
      {
        "header": "X-Forwarded-For",
        "value": "%{req.Header.Get \"Cf-Connecting-Ip\"}"
      },
      {
        "header": "X-Real-Ip",
        "value": "%{req.Header.Get \"Cf-Connecting-Ip\"}"
      }
    ]
  }
}  

